
<tr id="<%= dom_id stamp_paper %>">
  <td>
    <%= stamp_paper.owner %>
  </td>
  <td>
    <%= stamp_paper.tags %>
  </td>
  <td>
    <%= stamp_paper.sign_on_page %>
  </td>
  <td>
    <%= stamp_paper.notes %>
  </td>
  <td>
    <%= stamp_paper.note_on_page %>
  </td>
  <td>
    <%= link_to  t_common(:show), stamp_paper , {class: "btn btn-outline-primary"}%>
    <%= link_to  t_common(:edit), edit_stamp_paper_path(stamp_paper) , {class: "btn btn-outline-primary"} if policy(stamp_paper).edit?%>
  </td>
</tr>
